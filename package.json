    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.2",
    "vite-plugin-pwa": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/akhil-neoito/react-query-zustand-ts-vite-boilerplate.git"
